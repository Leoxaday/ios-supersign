<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.naoh.iossupersign.storage.mysql.mapper.DeviceMapper">

    <sql id="Base_Column_List">
        id,udid,apple_id,device_id,creat_time
    </sql>

    <insert id="insert" parameterType="com.naoh.iossupersign.model.po.AppleAccountPO">
    insert into apple_account (udid,apple_id,device_id,creat_time)
        values (
            #{udid,jdbcType=VARCHAR},
            #{apple_id,jdbcType=INTEGER},
            #{device_id,jdbcType=VARCHAR},
            #{creat_time,jdbcType=TIMESTAMP})
  </insert>

    <insert id="insertList" parameterType="java.util.List">
        insert into apple_account (udid,apple_id,device_id,creat_time)
        values
        <foreach collection="devicePOS" item="record" index="index"
                 separator=",">
            (#{record.udid,jdbcType=VARCHAR}, #{record.apple_id,jdbcType=INTEGER},
            #{record.device_id,jdbcType=VARCHAR}, #{record.creat_time,jdbcType=TIMESTAMP})
        </foreach>
    </insert>

</mapper>
